<div class="grid-container">
    {{#each artists}}
    <div class="card">
        <div class="card-body">
            <img src="{{images.[0].url}}" alt="" class="card-img-top" width="100px">
            <h3 class="card-title">{{name}}</h3>
        </div> 
        <div class="card-body">
        <a href="/albums/{{id}}" class="btn btn-primary">View Albums</a>
        </div>
    </div>
    {{/each}}
</div>